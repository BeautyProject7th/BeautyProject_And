<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.soma.beautyproject_android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERACT_ACROSS_USERS" />
    <uses-permission android:name="com.samsung.permission.HRM_EXT" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name="com.soma.beautyproject_android.Splash.AnalyticsApplication"
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:launchMode="singleTask"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash.SplashActivity_">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DressingTable.DressingTableActivity_"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.soma.beautyproject_android.DressingTable.More.MoreActivity_"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".DetailCosmetic.DetailCosmeticActivity_"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.soma.beautyproject_android.Setting.SettingActivity_"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.soma.beautyproject_android.Video.Video.PlayerViewDemoActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.soma.beautyproject_android.Skin.SkinTypeActivity_"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"/>
        <activity
            android:name="com.soma.beautyproject_android.Skin.SkinTroubleActivity_"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"/>
        <activity
            android:name="com.soma.beautyproject_android.Video.Video.VideoContentsActivity_"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <intent-filter>
                <!-- Sets the intent action to view the activity -->
                <action android:name="android.intent.action.VIEW" />
                <!-- Allows the link to be opened from a web browser -->
                <category android:name="android.intent.category.BROWSABLE" />
                <!-- Allows the deep link to be used without specifying the app name -->
                <category android:name="android.intent.category.DEFAULT" />
                <!-- URI callMyApp://video -->
                <data android:scheme="callmyapp" android:host="video"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.soma.beautyproject_android.Video.VideoList.VideoListActivity_"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.soma.beautyproject_android.Login.LoginActivity_"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.soma.beautyproject_android.DressingTable.CosmeticInfoRequest.RegistrationRequestActivity_"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.soma.beautyproject_android.DressingTable.CosmeticInfoRequest.CosmeticReport_"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity android:name=".DressingTable.CosmeticUpload.CosmeticUploadActivity_1"></activity>
        <activity android:name=".DressingTable.CosmeticUpload.CosmeticUploadActivity_2"></activity>
        <activity android:name=".DressingTable.CosmeticUpload.CosmeticUploadActivity_3"></activity>
        <activity android:name=".DressingTable.YourDressingTable.FindUserActivity_"></activity>
        <activity android:name=".DressingTable.YourDressingTable.YourDressingTableActivity_"></activity>
        <activity android:name=".DressingTable.CosmeticExpirationDate.CosmeticExpirationDateActivity"></activity>
        <activity android:name=".DetailCosmetic.ModifyCosmeticActivity_"></activity>
        <activity android:name=".Login.JoinActivity_"></activity>

        <activity android:name="com.soma.beautyproject_android.DressingTable.YourDressingTable.FollowingListActivity_"></activity>
        <activity android:name="com.soma.beautyproject_android.DressingTable.YourDressingTable.FollowerListActivity_"></activity>

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />
        <meta-data
            android:name="userhabitApiKey"
            android:value="@string/userhabit_api_key" />

        <service android:name="io.userhabit.service.main.service.UserhabitService" />
        <service
            android:name="com.soma.beautyproject_android.Utils.Push.MyFcmListenerService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service
            android:name="com.soma.beautyproject_android.Utils.Push.MyInstanceIDListenerService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>


        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="09ee142c27b4c4383b2de767468c4536de9d0feb" />
    </application>

</manifest>