<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:orientation="vertical" android:layout_width="match_parent"    android:layout_height="match_parent">    <android.support.design.widget.AppBarLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:theme="@style/AppTheme.AppBarOverlay"        android:id="@+id/appBarLayout2">        <include layout="@layout/toolbar_only_next" android:id="@+id/cs_toolbar" />    </android.support.design.widget.AppBarLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:orientation="vertical"        android:gravity="center_horizontal"        android:background="@color/colorWhite"        android:layout_alignParentTop="true"        android:layout_alignParentStart="true">        <!--<FrameLayout-->            <!--android:layout_width="match_parent"-->            <!--android:layout_height="116dp"-->            <!--android:layout_marginTop="62dp">-->            <!--<ImageView-->                <!--android:id="@+id/IV_user"-->                <!--android:layout_width="100dp"-->                <!--android:layout_height="100dp"-->                <!--android:layout_gravity="center"-->                <!--android:src="@drawable/user_profile"/>-->            <!--<ImageView-->                <!--android:layout_width="wrap_content"-->                <!--android:layout_height="wrap_content"-->                <!--android:src="@drawable/camera"-->                <!--android:layout_gravity="end"-->                <!--android:layout_marginRight="107dp"/>-->        <!--</FrameLayout>-->        <RelativeLayout            android:layout_marginTop="?attr/actionBarSize"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:gravity="center">            <TextView                android:id="@+id/TV_nickname"                android:layout_width="wrap_content"                android:layout_height="40dp"                android:gravity="center_vertical"                android:text="닉네임"                android:textColor="@color/colorAccentLight"                android:textStyle="bold"                android:textSize="13sp"                android:layout_marginLeft="38dp"                android:layout_marginRight="17.5dp"/>            <RelativeLayout                android:id="@+id/area_nickname"                android:layout_toRightOf="@id/TV_nickname"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginRight="50.5dp">                <EditText                    android:id="@+id/ET_nickname"                    android:layout_width="match_parent"                    android:layout_height="40dp"                    android:layout_marginRight="30dp"                    android:paddingLeft="10dp"                    android:paddingRight="10dp"                    android:background="@color/colorWhite"                    android:textColorHint="@color/colorGrayText"                    android:hint="닉네임을 입력하세요."                    android:textSize="13sp"                    android:textColor="@color/colorBlackText"/>                <RelativeLayout                    android:layout_width="30dp"                    android:layout_height="wrap_content"                    android:layout_alignTop="@+id/ET_nickname"                    android:layout_alignBottom="@id/ET_nickname"                    android:layout_alignParentRight="true"                    android:gravity="center">                    <ImageView                        android:id="@+id/BT_nicknameCheck"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:src="@drawable/ic_back_east"/>                </RelativeLayout>                <View                    android:id="@+id/line1"                    android:layout_below="@id/ET_nickname"                    android:layout_width="match_parent"                    android:layout_height="1.5dp"                    android:background="@color/colorGrayBackGround"/>                <TextView                    android:id="@+id/TV_nicknameResult"                    android:layout_below="@id/line1"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="닉네임 중복확인을 해주세요."                    android:textColor="@color/colorPrimary"                    android:textStyle="bold"                    android:textSize="13sp"                    android:layout_marginLeft="10dp"                    android:layout_marginTop="10dp"                    android:layout_marginBottom="15dp"/>            </RelativeLayout>            <TextView                android:id="@+id/TV_sex"                android:layout_below="@id/area_nickname"                android:layout_width="wrap_content"                android:layout_height="40dp"                android:layout_alignRight="@+id/TV_nickname"                android:gravity="center_vertical"                android:text="성별"                android:textColor="@color/colorAccentLight"                android:textStyle="bold"                android:textSize="13sp"/>            <LinearLayout                android:id="@+id/area_sex"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_below="@id/area_nickname"                android:layout_toRightOf="@id/TV_sex"                android:layout_marginLeft="17.5dp"                android:layout_marginBottom="15dp"                android:orientation="vertical">                <Spinner                    android:id="@+id/SP_sex"                    android:layout_width="wrap_content"                    android:layout_height="40dp"                    android:layout_marginRight="10dp"                    android:paddingLeft="10dp"                    android:background="@color/colorWhite"/>                <View                    android:layout_width="match_parent"                    android:layout_height="1.5dp"                    android:background="@color/colorGrayBackGround"/>            </LinearLayout>            <LinearLayout                android:id="@+id/area_birth"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_below="@id/area_sex"                android:layout_toRightOf="@id/TV_sex"                android:layout_marginLeft="17.5dp"                android:orientation="vertical">                <Spinner                    android:id="@+id/SP_birth"                    android:layout_width="wrap_content"                    android:layout_height="40dp"                    android:layout_marginRight="10dp"                    android:paddingLeft="10dp"                    android:background="@color/colorWhite"/>                <View                    android:layout_width="match_parent"                    android:layout_height="1.5dp"                    android:background="@color/colorGrayBackGround"/>            </LinearLayout>            <TextView                android:id="@+id/TV_birth"                android:layout_width="wrap_content"                android:layout_height="40dp"                android:gravity="center_vertical"                android:text="태어난 해"                android:textColor="@color/colorAccentLight"                android:textStyle="bold"                android:textSize="13sp"                android:layout_below="@+id/area_sex"                android:layout_toStartOf="@+id/area_birth" />        </RelativeLayout>    </LinearLayout></RelativeLayout>