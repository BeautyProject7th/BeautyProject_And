<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/cell_search_result_video"
    android:orientation="vertical"
    android:layout_width="344dp"
    android:layout_height="wrap_content"
    android:background="@drawable/ic_card"
    android:layout_gravity="center_horizontal"
    android:layout_marginTop="8dp"
    android:layout_marginLeft="8dp"
    android:layout_marginRight="8dp"
    android:padding="15dp">
    <LinearLayout
        android:id="@+id/LL_video_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="영상 검색 결과"
            android:textStyle="bold"
            android:textSize="13sp"
            android:textColor="#4a494a"
            android:layout_marginBottom="7dp"/>
        <Space
            android:layout_weight="1"
            android:layout_width="0dp"
            android:layout_height="match_parent" />
        <Button
            android:id="@+id/BT_video_more"
            android:layout_width="40dp"
            android:layout_height="14.5dp"
            android:text="더보기"
            android:background="@null"
            android:textSize="13sp"
            android:textColor="#a0a0a0"
            android:textStyle="bold"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp">
        <TextView
            android:id="@+id/TV_video_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="영상 제목"
            android:textStyle="bold"
            android:textSize="10sp"
            android:textColor="#4a494a"/>

    </LinearLayout>
    <LinearLayout
        android:id="@+id/LL_video_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageView
            android:layout_width="9dp"
            android:layout_height="9dp"
            android:background="@drawable/ic_play_circle_outline"/>
        <TextView
            android:id="@+id/TV_view_cnt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="#a6a6a6"
            android:textSize="9sp"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="4.5dp"/>
        <ImageView
            android:layout_width="1.5dp"
            android:layout_height="9dp"
            android:background="@drawable/ic_line_vertical"
            android:layout_marginRight="4dp"/>
        <ImageView
            android:layout_width="9dp"
            android:layout_height="9dp"
            android:background="@drawable/ic_upload"/>
        <TextView
            android:id="@+id/TV_upload_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="#a6a6a6"
            android:textSize="9sp"
            android:layout_marginLeft="2dp"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/LL_video_total"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_gravity="center">
        <LinearLayout
            android:id="@+id/LL_youtuber_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/RL_youtuber_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="3.5dp">
                <ImageView
                    android:layout_width="13dp"
                    android:layout_height="9dp"
                    android:background="@drawable/ic_crown"/>
                <ImageView
                    android:id="@+id/IV_youtuber"
                    android:layout_width="61dp"
                    android:layout_height="61dp"
                    android:background="@drawable/ic_youtuber"
                    android:layout_marginLeft="7dp"></ImageView>
            </RelativeLayout>
            <TextView
                android:id="@+id/TV_youtuber_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textColor="#fc617e"
                android:textSize="10sp"
                android:layout_gravity="center"
                android:layout_marginBottom="4.5dp"/>
            <LinearLayout
                android:id="@+id/LL_youtuber_profile_skin"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/IV_skin_type"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@null"/>
                <Space
                    android:layout_width="5dp"
                    android:layout_height="match_parent" />
                <ImageView
                    android:id="@+id/IV_skin_trouble_1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"

                    android:background="@null"/>
                <Space
                    android:layout_width="5dp"
                    android:layout_height="match_parent" />
                <ImageView
                    android:id="@+id/IV_skin_trouble_2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@null"/>
                <Space
                    android:layout_width="5dp"
                    android:layout_height="match_parent" />
                <ImageView
                    android:id="@+id/IV_skin_trouble_3"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@null"/>
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:id="@+id/IV_video"
            android:layout_width="184dp"
            android:layout_height="103.5dp"
            android:layout_marginLeft="15dp"
            android:background="@null">
        </ImageView>
    </LinearLayout>


</LinearLayout>