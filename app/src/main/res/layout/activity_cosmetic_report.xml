<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:orientation="vertical" android:layout_width="match_parent"    android:layout_height="match_parent">    <android.support.design.widget.AppBarLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:theme="@style/AppTheme.AppBarOverlay"        android:id="@+id/appBarLayout2">        <include layout="@layout/toolbar_cosmetic_report" android:id="@+id/cs_toolbar" />    </android.support.design.widget.AppBarLayout>    <RelativeLayout        android:id="@+id/whole"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:orientation="vertical"        android:layout_below="@+id/appBarLayout2"        android:layout_alignParentStart="true">        <LinearLayout            android:id="@+id/cosmetic_info"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="#FFFFFF"            android:padding="15dp"            android:gravity="center_vertical">            <ImageView                android:id="@+id/IV_product"                android:layout_width="80dp"                android:layout_height="80dp"                android:background="#555555"                android:layout_alignParentTop="true"                android:layout_alignParentStart="true"                android:layout_marginRight="15dp"/>            <LinearLayout                android:id="@+id/cosmetic_basic_input"                android:layout_centerVertical="true"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="vertical">                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="horizontal">                    <TextView                        android:id="@+id/TV_brand"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="브랜드명"                        android:textSize="14sp"                        android:textColor="@color/colorGrayText"                        android:layout_marginRight="5dp"/>                    <TextView                        android:id="@+id/TV_main_category"                        android:text="메인카테고리"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:textSize="14sp"                        android:textColor="@color/colorGrayText"                        android:layout_marginBottom="5dp"/>                    <TextView                        android:text=" - "                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:textSize="14sp"                        android:textColor="@color/colorGrayText"                        android:layout_marginBottom="5dp"/>                    <TextView                        android:id="@+id/TV_sub_category"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="서브카테고리"                        android:textSize="14sp"                        android:textColor="@color/colorGrayText"                        android:layout_marginBottom="5dp"/>                </LinearLayout>                <TextView                    android:id="@+id/TV_product_name"                    android:layout_width="wrap_content"                    android:layout_height="0dp"                    android:layout_weight="1"                    android:text="상품명"                    android:textSize="16sp"                    android:textColor="@color/colorBlack"/>            </LinearLayout>        </LinearLayout>        <LinearLayout            android:id="@+id/troublecheck"            android:layout_below="@+id/cosmetic_info"            android:layout_marginTop="10dp"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="@color/colorWhite"            android:padding="15dp"            android:orientation="vertical">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="수정이 필요한 항목"                android:textSize="16sp"                android:textColor="@color/colorBlack"                android:layout_marginBottom="10dp"/>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="horizontal">                <CheckBox                    android:id="@+id/CB_cos_img"                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="wrap_content"                    android:text="화장품 사진"                    android:textSize="14sp"                    android:textColor="@color/colorGrayText"                    android:padding="10dp"                    android:textAlignment="center"                    android:layout_marginRight="10dp"                    android:background="@drawable/pointbox"/>                <CheckBox                    android:id="@+id/CB_cos_name"                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="wrap_content"                    android:text="화장품 이름"                    android:textSize="14sp"                    android:textColor="@color/colorGrayText"                    android:padding="10dp"                    android:textAlignment="center"                    android:layout_marginLeft="10dp"                    android:background="@drawable/pointbox"/>            </LinearLayout>            <LinearLayout                android:layout_marginTop="10dp"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="horizontal">                <CheckBox                    android:id="@+id/CB_category"                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="wrap_content"                    android:text="카테고리 정보"                    android:textSize="14sp"                    android:textColor="@color/colorGrayText"                    android:padding="10dp"                    android:textAlignment="center"                    android:layout_marginRight="10dp"                    android:background="@drawable/pointbox"/>                <CheckBox                    android:id="@+id/CB_bra_name"                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="wrap_content"                    android:text="브랜드 이름"                    android:textSize="14sp"                    android:textColor="@color/colorGrayText"                    android:padding="10dp"                    android:textAlignment="center"                    android:layout_marginLeft="10dp"                    android:background="@drawable/pointbox"/>            </LinearLayout>        </LinearLayout>        <LinearLayout            android:layout_above="@+id/last"            android:layout_below="@+id/troublecheck"            android:layout_marginTop="2dp"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="vertical"            android:padding="15dp"            android:background="@color/colorWhite">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="세부 수정 사항"                android:textSize="16sp"                android:textColor="@color/colorBlack"                android:layout_marginBottom="10dp"/>            <EditText                android:id="@+id/ET_detail"                android:layout_marginTop="3dp"                android:layout_width="match_parent"                android:layout_height="match_parent"                android:hint="자세한 사항을 작성해주세요.\n(최대 200글자)"                android:gravity="top"                android:maxLength="200"                android:textSize="14sp"                android:padding="15dp"                android:background="@drawable/edittext_review" />        </LinearLayout>        <LinearLayout            android:id="@+id/last"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:background="@color/colorWhite"            android:layout_alignParentBottom="true">            <Button                android:id="@+id/BT_send"                android:layout_gravity="center_horizontal"                android:layout_width="match_parent"                android:layout_height="50dp"                android:layout_margin="10dp"                android:text="전송"                android:textColor="#FFFFFF"                android:textSize="16sp"                android:background="@drawable/btn_ver2" />        </LinearLayout>    </RelativeLayout></RelativeLayout>