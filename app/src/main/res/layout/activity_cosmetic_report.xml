<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:orientation="vertical" android:layout_width="match_parent"    android:layout_height="match_parent">    <android.support.design.widget.AppBarLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:theme="@style/AppTheme.AppBarOverlay"        android:id="@+id/appBarLayout2">        <include layout="@layout/toolbar_only_back" android:id="@+id/cs_toolbar" />    </android.support.design.widget.AppBarLayout>    <ScrollView        android:layout_width="match_parent"        android:layout_height="match_parent">    </ScrollView>    <RelativeLayout        android:id="@+id/whole"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:orientation="vertical"        android:layout_alignParentBottom="true"        android:layout_alignParentStart="true"        android:layout_marginTop="?attr/actionBarSize">        <LinearLayout            android:id="@+id/cosmetic_info"            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="vertical"            android:background="@color/colorWhite"            android:gravity="center"            android:paddingTop="15dp"            android:paddingBottom="15dp">            <ImageView            android:id="@+id/IV_product"            android:layout_width="90dp"            android:layout_height="90dp" />            <TextView                android:id="@+id/TV_brand"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="브랜드명"                android:textSize="12sp"                android:textColor="@color/colorBlack"                android:layout_marginTop="14dp"/>            <LinearLayout                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:orientation="horizontal"                android:layout_marginTop="4dp">                <TextView                    android:id="@+id/TV_main_category"                    android:text="메인카테고리"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:textSize="11sp"                    android:textColor="@color/colorGrayText"/>                <TextView                    android:text="/"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:textSize="11sp"                    android:textColor="@color/colorGrayText"/>                <TextView                    android:id="@+id/TV_sub_category"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:textSize="11sp"                    android:textColor="@color/colorGrayText"                    android:text="서브카테고리"/>            </LinearLayout>            <TextView                android:id="@+id/TV_product_name"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginTop="4dp"                android:text="상품명"                android:gravity="center"                android:textColor="@color/colorBlack"                android:textStyle="bold"                android:textSize="12sp"/>            <TextView                android:id="@+id/TV_product_price"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_marginTop="7.5dp"                android:text="가격"                android:gravity="center"                android:textColor="#a1a1a1"                android:textSize="12sp"/>        </LinearLayout>        <RelativeLayout            android:id="@+id/troublecheck"            android:layout_below="@+id/cosmetic_info"            android:layout_marginTop="5dp"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:background="@color/colorWhite"            android:padding="15dp"            android:orientation="vertical">            <TextView                android:id="@+id/explain"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="어떤 화장품 정보가 잘못되었나요?"                android:textSize="13.5sp"                android:textStyle="bold"                android:textColor="#fb617e"                android:layout_marginBottom="10dp"/>            <LinearLayout                android:id="@+id/checkbox_list1"                android:layout_below="@+id/explain"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="horizontal">                <CheckBox                    android:id="@+id/CB_cos_img"                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="wrap_content"                    android:button="@drawable/custom_checkbox"                    android:paddingLeft="10dp"                    android:text="화장품 사진"                    android:textSize="12sp"                    android:textColor="@color/colorBlack"                    android:layout_marginRight="10dp"                    android:layout_marginLeft="19dp"/>                <CheckBox                    android:id="@+id/CB_cos_name"                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="wrap_content"                    android:button="@drawable/custom_checkbox"                    android:paddingLeft="10dp"                    android:text="화장품 이름"                    android:textSize="12sp"                    android:textColor="@color/colorBlack"                    android:padding="5dp"/>            </LinearLayout>            <LinearLayout                android:layout_below="@id/checkbox_list1"                android:id="@+id/checkbox_list2"                android:layout_marginTop="10dp"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="horizontal">                <CheckBox                    android:id="@+id/CB_bra_name"                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="wrap_content"                    android:button="@drawable/custom_checkbox"                    android:paddingLeft="10dp"                    android:text="브랜드 이름"                    android:textSize="12sp"                    android:textColor="@color/colorBlack"                    android:layout_marginRight="10dp"                    android:layout_marginLeft="19dp"/>                <CheckBox                    android:id="@+id/CB_category"                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="wrap_content"                    android:button="@drawable/custom_checkbox"                    android:paddingLeft="10dp"                    android:text="카테고리 정보"                    android:textSize="12sp"                    android:textColor="@color/colorBlack"                    android:padding="5dp"/>            </LinearLayout>            <TextView                android:id="@+id/TV_detail"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="기타 요청 사항"                android:textSize="13.5sp"                android:textStyle="bold"                android:textColor="#fb617e"                android:layout_marginTop="15dp"                android:layout_marginBottom="5dp"                android:layout_below="@id/checkbox_list2"/>            <Button                android:id="@+id/BT_send"                android:layout_gravity="center_horizontal"                android:layout_width="match_parent"                android:layout_height="45dp"                android:layout_marginTop="15dp"                android:layout_alignParentBottom="true"                android:text="완료"                android:textColor="#FFFFFF"                android:textSize="16sp"                android:background="@drawable/rectangle_18_copy_2" />            <EditText                android:id="@+id/ET_detail"                android:layout_width="match_parent"                android:layout_height="match_parent"                android:hint="매화에게 요청하실 사항을 작성해주세요!\n(최대 200글자까지 작성이 가능합니다.)"                android:gravity="top"                android:maxLength="200"                android:textSize="12sp"                android:padding="15dp"                android:background="@drawable/cosmetic_user_rectangle"                android:layout_below="@+id/TV_detail"                android:layout_alignParentStart="true"                android:layout_marginBottom="55dp"/>        </RelativeLayout>    </RelativeLayout></RelativeLayout>