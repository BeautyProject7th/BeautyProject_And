<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:orientation="vertical" android:layout_width="match_parent"    android:layout_height="match_parent">    <android.support.design.widget.AppBarLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:theme="@style/AppTheme.AppBarOverlay"        android:id="@+id/appBarLayout2">        <include layout="@layout/toolbar_cosmetic_report" android:id="@+id/cs_toolbar" />    </android.support.design.widget.AppBarLayout>    <RelativeLayout        android:id="@+id/whole"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:orientation="vertical"        android:layout_below="@+id/appBarLayout2"        android:layout_alignParentStart="true">        <LinearLayout            android:id="@+id/cosmetic_info"            android:layout_width="match_parent"            android:layout_height="250dp"            android:orientation="vertical"            android:background="@color/colorWhite"            android:padding="15dp"            android:gravity="center">            <ImageView                android:id="@+id/IV_product"                android:layout_width="wrap_content"                android:layout_height="100dp"                android:background="@color/colorWhite"                android:layout_marginBottom="15dp"/>            <TextView                android:id="@+id/TV_product_name"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="상품명"                android:gravity="center"                android:textSize="13sp"                android:textStyle="bold"                android:textColor="@color/colorBlack"                android:layout_marginBottom="10dp"/>            <TextView                android:id="@+id/TV_brand"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="브랜드명"                android:textSize="11sp"                android:textColor="@color/colorPrimaryDark"                android:layout_marginBottom="8dp"/>            <LinearLayout                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:orientation="horizontal">                <TextView                    android:id="@+id/TV_main_category"                    android:text="메인카테고리"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:textSize="11.5sp"                    android:textColor="@color/colorGrayText"/>                <TextView                    android:text=" - "                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:textSize="11.5sp"                    android:textColor="@color/colorGrayText"/>                <TextView                    android:id="@+id/TV_sub_category"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="서브카테고리"                    android:textSize="11.5sp"                    android:textColor="@color/colorGrayText"/>            </LinearLayout>        </LinearLayout>        <RelativeLayout            android:id="@+id/troublecheck"            android:layout_below="@+id/cosmetic_info"            android:layout_marginTop="5dp"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:background="@color/colorWhite"            android:padding="13dp"            android:orientation="vertical">            <TextView                android:id="@+id/explain"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_centerHorizontal="true"                android:text="어떤 화장품 정보가 잘못되었나요?"                android:textSize="16sp"                android:textStyle="bold"                android:textColor="@color/colorPrimary"                android:layout_marginTop="10dp"                android:layout_marginBottom="18dp"/>            <LinearLayout                android:id="@+id/checkbox_list1"                android:layout_below="@+id/explain"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="horizontal"                android:paddingLeft="15dp"                android:paddingRight="15dp">                <CheckBox                    android:id="@+id/CB_cos_img"                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="wrap_content"                    android:text="화장품 사진"                    android:textSize="14.5sp"                    android:textStyle="bold"                    android:textColor="@color/colorPrimaryDark"                    android:padding="5dp"                    android:layout_marginRight="10dp"/>                <CheckBox                    android:id="@+id/CB_cos_name"                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="wrap_content"                    android:text="화장품 이름"                    android:textSize="14.5sp"                    android:textStyle="bold"                    android:textColor="@color/colorPrimaryDark"                    android:padding="5dp"/>            </LinearLayout>            <LinearLayout                android:layout_below="@id/checkbox_list1"                android:id="@+id/checkbox_list2"                android:layout_marginTop="10dp"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="horizontal"                android:paddingLeft="15dp"                android:paddingRight="15dp">                <CheckBox                    android:id="@+id/CB_bra_name"                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="wrap_content"                    android:text="브랜드 이름"                    android:textSize="14.5sp"                    android:textStyle="bold"                    android:textColor="@color/colorPrimaryDark"                    android:padding="5dp"                    android:layout_marginRight="10dp"/>                <CheckBox                    android:id="@+id/CB_category"                    android:layout_width="0dp"                    android:layout_weight="1"                    android:layout_height="wrap_content"                    android:text="카테고리 정보"                    android:textSize="14.5sp"                    android:textStyle="bold"                    android:textColor="@color/colorPrimaryDark"                    android:padding="5dp"/>            </LinearLayout>            <EditText                android:id="@+id/ET_detail"                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_below="@+id/checkbox_list2"                android:layout_above="@+id/BT_send"                android:layout_marginTop="15dp"                android:hint="자세한 사항을 작성해주세요.\n(최대 200글자)"                android:gravity="top"                android:maxLength="200"                android:textSize="14sp"                android:padding="15dp"                android:background="@drawable/edittext_review" />            <Button                android:id="@+id/BT_send"                android:layout_gravity="center_horizontal"                android:layout_width="match_parent"                android:layout_height="50dp"                android:layout_marginTop="15dp"                android:layout_alignParentBottom="true"                android:text="전송"                android:textColor="#FFFFFF"                android:textSize="16sp"                android:background="@drawable/large_btn_shadow" />        </RelativeLayout>    </RelativeLayout></RelativeLayout>